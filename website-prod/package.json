{
  "name": "pi-projects-ui",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrasm91/pi-projects.git"
  },
  "bugs": {
    "url": "https://github.com/jrasm91/pi-projects/issues"
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/stats-es2015.json",
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "npm run start:dev",
    "start:dev": "ng serve --proxy-config=proxy.json --aot --port 4200",
    "build:dev": "ng build --prod --aot --stats-json",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint docs --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@nebular/theme": "4.4.0",
    "bootstrap": "4.3.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.1",
    "echarts": "^4.9.0",
    "intl": "1.2.5",
    "nebular-icons": "1.1.0",
    "ng2-completer": "2.0.8",
    "ngx-echarts": "^5.1.2",
    "node-sass": "^4.13.1",
    "normalize.css": "6.0.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.5.2",
    "tslib": "^1.11.1",
    "typeface-exo": "0.0.22",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.6",
    "@angular/cli": "^8.3.26",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "8.0.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@compodoc/compodoc": "1.0.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@types/d3-color": "1.0.5",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "6.0.90",
    "codelyzer": "^5.2.2",
    "conventional-changelog-cli": "1.3.4",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "^10.2.2",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.2",
    "rimraf": "2.6.1",
    "stylelint": "7.13.0",
    "ts-node": "3.2.2",
    "tslint": "^5.20.1",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "lint-staged": {
    "./src/**/*.json": "npx sort-json --",
    "./src/**/*.ts": "tslint --fix --project ./tsconfig.json"
  }
}
